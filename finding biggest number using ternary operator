package ConditonalOperators;
import java.util.*;
public class TerenaryOperator {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter 2 numbers :");
		int a = sc.nextInt(); 
		int b = sc.nextInt();	
		int c = (a>b) ? a : b;		
System.out.println("bigger number = "+ c);
	}
}
