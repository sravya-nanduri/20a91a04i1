package cond;
public class weight_age {
	public static void main(String[] args) {
		int age = 62;
		int weight = 75;
		if(age > 18)
		{
		  if( weight > 40)		  
			  System.out.println("Eligible");
		  else
			  System.out.println("Not Eligible");	  		  
		}
		else
			System.out.println("Age should be more than 18");
	} }
